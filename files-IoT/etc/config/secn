
config secn 'asterisk'
	option codec1 'gsm'
	option codec2 'ulaw'
	option codec3 'alaw'
	option host 'sip.myhost.com'
	option fromdomain 'sip.myhost.com'
	option dialout '#'
	option externip '0.0.0.0'
	option reghost 'sip.myhost.com'
	option softph 'OFF'
	option username 'myuser'
	option fromusername 'myuser'
	option enable '0'
	option register '0'
	option enablenat '0'
	option enable_ast '0'

config secn 'accesspoint'
	option usreg_domain '1'
	option maxassoc '30'
	option ssid 'dragino2'
	option passphrase 'dragino-dragino'
	option CHANNEL '4'
	option ap_disable '0'
	option encryption 'WPA2'

config secn 'mesh'
	option mesh_enable 'unchecked'

config secn 'dhcp'
	option domain 'lan'
	option maxleases '40'
	option dns '8.8.8.8'
	option dns2 '8.8.4.4'
	option enable 'checked'
	option subnet '255.255.255.0'
	option leaseterm '7200'
	option dhcp_auth 'unchecked'
	option startip '10.130.1.200'
	option endip '10.130.1.240'
	option router '10.130.1.1'
	option FallbackIP 'enable'

config secn 'mpgw'
	option mode 'OFF'

config secn 'http'
	option limitip '0'
	option enssl '0'
	option pw_preset '1'
	option auth 'checked'

config secn 'ivr'
	option pin '1234'

config secn 'modem'
	option enabled '0'
	option service 'cdma'

config secn 'wan'
	option wanport 'Ethernet'
	option ethwanmode 'DHCP'

